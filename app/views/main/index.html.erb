
<!-- START HOMEPAGE DESIGN AREA -->
  <header id="home" class="welcome-area">
    <div class="header-top-area">
      <div class="container">
        <div class="row">
          <div class="col-md-3">
            <% if notice %>
              <%= notice %>
            <% end %>
            <% if alert %>
              <%= alert %>
            <% end %>
            <!-- START LOGO DESIGN AREA -->
            <div class="logo">
            	<%= link_to(root_path) do %>
                <%= image_tag "team/logo_nav.png", class: "", alt: "" %>
              <% end %>
            </div>
            <!-- END LOGO DESIGN AREA -->
          </div>
          <div class="col-md-9">
            <!-- START MENU DESIGN AREA -->
            <div class="mainmenu">
              <div class="navbar navbar-nobg">
                <div class="navbar-header">
                  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                  </button>
                </div>
                <div class="navbar-collapse collapse">
                  <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a class="smoth-scroll" href="#home"><%=I18n.t("index.navbar.home")%> <div class="ripple-wrapper"></div></a></li>
                    <li><a class="smoth-scroll" href="#about"><%=I18n.t("index.navbar.video")%></a></li>
                    <li><a class="smoth-scroll" href="#service"><%=I18n.t("index.navbar.pibox")%></a></li>
                    <li><a class="smoth-scroll" href="#pigrow"><%=I18n.t("index.navbar.pigrow")%></a></li>
                    <li><a class="smoth-scroll" href="#work"><%=I18n.t("index.navbar.plants")%></a></li>
                    <li><a class="smoth-scroll" href="#blog"><%=I18n.t("index.navbar.forum")%></a></li>
                    <li><a class="smoth-scroll" href="#team"><%=I18n.t("index.navbar.team")%></a></li>
                    <li><a class="smoth-scroll" href="#contact"><%=I18n.t("index.navbar.contact")%></a></li>
                  </ul>
                </div>
              </div>
            </div>
          <!-- END MENU DESIGN AREA -->
          </div>
        </div>
      </div>
    </div>
    <div class="carousel fade-carousel slide" data-ride="carousel" data-interval="4000" id="bs-carousel">
